{
  "name": "Resolve Did Web",
  "description": "Type 'did:web' plus a did:web value into the address bar to resolve did in a new tab.",
  "version": "0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "service-worker.js"
  },
  "omnibox": { "keyword": "did:web" },
  "action": {
    "default_icon": {
      "16": "did16.png",
      "32": "did32.png",
      "48": "did48.png",
      "128": "did128.png"
    }
  },
  "icons": {
    "16": "did16.png",
    "32": "did32.png",
    "48": "did48.png",
    "128": "did128.png"
  },
  "permissions": ["tabs", "activeTab"]
}
